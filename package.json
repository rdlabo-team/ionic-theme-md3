{
  "name": "@rdlabo/ionic-theme-ios26",
  "version": "1.0.0",
  "description": "iOS26 CSS Theme for Ionic Framework",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dist/css/*": "./dist/css/*",
    "./src/styles/*": "./src/styles/*"
  },
  "files": [
    "dist",
    "src",
    "package.json"
  ],
  "scripts": {
    "build": "npm run build:css && npm run build:ts",
    "build:css": "rm -rf dist/css && sass src/styles:dist/css --style=compressed --no-source-map",
    "build:ts": "tsc",
    "build:demo": "npm run build && cd demo && npm install && npm run build -- --configuration=production",
    "prepare": "husky install",
    "lint": "prettier --check \"./**/*.{scss,ts}\" &&  prettier --parser angular --check \"./**/*.html\"",
    "fmt": "prettier --write \"./**/*.{scss,ts}\" &&  prettier --parser angular --write \"./**/*.html\"",
    "release": "np --no-tests",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rdlabo-team/ionic-theme-ios26.git"
  },
  "keywords": [
    "ionic",
    "framework",
    "theme",
    "css",
    "ios26"
  ],
  "author": "Masahiko Sakakibara <sakakibara@rdlabo.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdlabo-team/ionic-theme-ios26/issues"
  },
  "homepage": "https://github.com/rdlabo-team/ionic-theme-ios26#readme",
  "devDependencies": {
    "@ionic/angular": "^8.7.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.1",
    "np": "^10.2.0",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@ionic/core": "> 8.0.0"
  }
}
